server {
                listen  3377;
                server_name barber_api;

                #logs
                access_log /var/log/nginx/barber_api.access.log;
                error_log /var/log/nginx/barber_api.error.log;

                location / {
                         proxy_pass http://unix:/home/marcos-viana/api/barber_api/barber_api.sock;
                }
        }